--update and draw

function _update()
	player:update()
	if sw_running then
		frame_total += (1 / 2) ^ 16
		if time() - sw_anim > .2 then
			sw_anim = time()
			sw_sp -= 1
			if sw_sp < 9 then
				sw_sp = 12
			end
		end
	end
	if collision(player, bit) then
		bit_collected()
	end
	if btnp(🅾️) and bits_left == 0 then
		restart_game()
	end
end

function _draw()
	cls()
	map(0, 0)
	rectfill(0, 0, 128, 7, 0)
	spr(sw_sp, 80, 0)
	print(get_time_from_frames(tostr(frame_total, 2)), 90, 1, 7)
	spr(player.sp, player.x, player.y, 1, 1, player.flp)
	bit:draw()
	if bits_left == 0 then
		print("you did it", 54, 50, 7)
		print("press 🅾️ to play again", 20, 80, 7)
	elseif bits_left == 10 then
		title_x += 1
		if title_x == 130 then
			title_x = -30
		end
		print("get the lemon", title_x, 40, 7)
	end
end

