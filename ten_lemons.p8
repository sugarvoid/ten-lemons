--player

player = {
	sp = 1,
	x = 8,
	y = 40,
	w = 8,
	h = 8,
	flp = false,
	jumps_lefts = 2,
	dx = 0,
	dy = 0,
	max_dx = 2,
	max_dy = 3,
	acc = 0.5,
	boost = 2,
	anim = 0,
	idle = true,
	jumping = false,
	falling = false,
	update = function(self)
		self.dy += gravity
		self.dx *= friction

		if btn(⬅️) then
			self.dx -= self.acc
			self.flp = true
		end
		if btn(➡️) then
			self.dx += self.acc
			self.flp = false
		end
		--jump
		if btnp(❎) and self.jumps_lefts > 0 then
			self.jumps_lefts -= 1
			self.dy = 0
			self.dy -= self.boost
			self.landed = false
			sfx(0)
		end
		--check collision down
		if self.dy > 0 then
			self.falling = true
			self.landed = false
			self.jumping = false

			self.dy = limit_speed(self.dy, self.max_dy)

			if collide_map(player, "down", 0) then
				self.landed = true
				self.jumps_lefts = 2
				self.falling = false
				self.dy = 0
				self.y -= (self.y + self.h) % 8
			end
		elseif self.dy < 0 then
			self.jumping = true
		end

		--collision left and right
		if self.dx < 0 then
			self.dx = limit_speed(self.dx, self.max_dx)
			if collide_map(self, "left", 1) then
				self.dx = 0
			end
		elseif self.dx > 0 then
			self.dx = limit_speed(self.dx, player.max_dx)
			if collide_map(self, "right", 1) then
				self.dx = 0
			end
		end

		if self.y > 126 then
			sfx(1)
			self.y = -1
		end

		if self.x < 0 then
			sfx(1)
			self.x = 127
		elseif self.x > 127 then
			sfx(1)
			self.x = 0
		end
		self.x += self.dx
		self.y += self.dy
		if self.jumping then
			pal(8, 14)
		else
			pal()
		end
		self:animate()
	end,
	animate = function(self)
		if self.jumping then
			self.sp = 3
		elseif self.falling then
			self.sp = 4
		elseif time() - self.anim > .3 then
			self.anim = time()
			self.sp += 1
			if self.sp > 2 then
				self.sp = 1
			end
		end
	end
}

function limit_speed(num, maximum)
	return mid(-maximum, num, maximum)
end
