--bits

all_bits = {}

function get_bit_pos(num)
	return { bit_pos[num][1], bit_pos[num][2] }
end

bit_pos = {
	{ 32, 8 },
	{ 64, 8 },
	{ 96, 8 },

	{ 24, 48 },
	{ 56, 48 },
	{ 88, 48 },

	{ 32, 88 },
	{ 64, 88 },
	{ 96, 88 }
}

bit_order = { 5, 4, 6, 1, 8, 9, 5, 8, 4, 5 }

bit = {
	x = 0,
	y = 0,
	draw = function(self)
		spr(5, self.x, self.y)
	end,
	move = function(self, pos)
		self.x = pos[1]
		self.y = pos[2]
	end
}

function bit_collected()
	sfx(2)
	bits_left -= 1
	if bits_left == 9 then
		sfx(4)
		sw_running = true
	end
	if round < #bit_order then
		round += 1
		bit:move(get_bit_pos(bit_order[round]))
	else
		bit:move({ 0, -20 })
		sfx(3)
		sw_running = false
		--game over
	end
end

